<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hennesa.calculator.MainActivity">

    <Button
        android:id="@+id/button29"
        android:layout_width="242dp"
        android:layout_height="57dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:text="="
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button26"
        android:onClick="calcBtnPressed" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="327dp"
        android:layout_height="40dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="45dp"
        android:text="0"
        android:textAlignment="textEnd"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteY="47dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:text="2"
        app:layout_constraintBaseline_toBaselineOf="@+id/btn_num1"
        app:layout_constraintLeft_toRightOf="@+id/btn_num1"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:text="3"
        app:layout_constraintBaseline_toBaselineOf="@+id/button"
        app:layout_constraintLeft_toRightOf="@+id/button"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/btn_add"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:onClick="calcBtnPressed"
        android:text="AC"
        app:layout_constraintBaseline_toBaselineOf="@+id/button3"
        app:layout_constraintLeft_toRightOf="@+id/button3" />

    <Button
        android:id="@+id/btn_num1"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:text="1"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="98dp"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button17"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_num1"
        android:onClick="calcBtnPressed"
        />

    <Button
        android:id="@+id/button18"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:text="5"
        app:layout_constraintLeft_toRightOf="@+id/button17"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button19"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:text="6"
        app:layout_constraintLeft_toRightOf="@+id/button18"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button20"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:onClick="calcBtnPressed"
        android:text="+"
        app:layout_constraintLeft_toRightOf="@+id/button19"
        app:layout_constraintTop_toBottomOf="@+id/btn_add" />

    <Button
        android:id="@+id/button21"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:text="7"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button17"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button22"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:text="8"
        app:layout_constraintLeft_toRightOf="@+id/button21"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button18"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button23"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:text="9"
        app:layout_constraintLeft_toRightOf="@+id/button22"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button19"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button24"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:onClick="calcBtnPressed"
        android:text="-"
        app:layout_constraintLeft_toRightOf="@+id/button23"
        app:layout_constraintTop_toBottomOf="@+id/button20" />

    <Button
        android:id="@+id/button25"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:text="/"
        android:onClick="calcBtnPressed"
        app:layout_constraintLeft_toRightOf="@+id/button29"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button2"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:onClick="calcBtnPressed"
        android:text="x"
        app:layout_constraintLeft_toRightOf="@+id/button27"
        app:layout_constraintTop_toBottomOf="@+id/button24" />

    <Button
        android:id="@+id/button27"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="del"
        app:layout_constraintLeft_toRightOf="@+id/button26"
        app:layout_constraintTop_toBottomOf="@+id/button23"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button28"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="."
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button21"
        android:onClick="calcBtnPressed"/>

    <Button
        android:id="@+id/button26"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintLeft_toRightOf="@+id/button28"
        app:layout_constraintTop_toBottomOf="@+id/button22"
        android:onClick="calcBtnPressed"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="329dp"
        android:layout_height="21dp"
        android:layout_marginTop="16dp"
        android:text=" "
        android:textAlignment="textEnd"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="23dp" />
</android.support.constraint.ConstraintLayout>
